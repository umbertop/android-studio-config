<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>Declaration</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'Alt+F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Parameter Info">
        <Item>
          <IdeaActionID>ParameterInfo</IdeaActionID>
          <ShortCut>'Ctrl+P'</ShortCut>
          <Description>Parameter Info</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Quick Definition">
        <Item>
          <IdeaActionID>QuickImplementations</IdeaActionID>
          <ShortCut>'Ctrl+Maiusc+I'</ShortCut>
          <Description>Quick Definition</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'app'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'app'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Logcat">
        <Item>
          <IdeaActionID>ActivateLogcatToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>Logcat</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>